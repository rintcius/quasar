{
    "name": "use type_of function for non-datetimes",
    "NB": "Expected results not verified for any connector.",
    "backends": {
        "lwc_local":         "pendingIgnoreFieldOrder",
        "mimir":             "pendingIgnoreFieldOrder",
        "mongodb_3_4_13":    "pending",
        "mongodb_3_6":       "pending"
    },
    "data": "types.data",
    "query": "select foo ?? \"nope\" as foo, type_of(foo ?? 42) as type from `types.data`",
    "predicate": "exactly",
    "ignoreResultOrder": true,
    "expected": [
      { "foo": 11, "type": "number" },
      { "foo": 1.12345, "type": "number" },
      { "foo": "13", "type": "string" },
      { "foo": "zap", "type": "string" },
      { "foo": true, "type": "boolean" },
      { "foo": false, "type": "boolean" },
      { "foo": null, "type": "null" },
      { "foo": { "$offsetdatetime": "2009-02-13T23:31:30.011+12:15:18" }, "type": "offsetdatetime" },
      { "foo": { "$offsetdate": "2009-02-13+12:15:18" }, "type": "offsetdate" },
      { "foo": { "$offsettime": "23:31:30.011+12:15:18" }, "type": "offsettime" },
      { "foo": { "$localdatetime" : "2009-02-13T23:31:30.011" }, "type": "localdatetime" },
      { "foo": { "$localdate" : "2009-02-13" }, "type": "localdate" },
      { "foo": { "$localtime" : "23:31:30.011" }, "type": "localtime" },
      { "foo": { "$interval" : "P7DT12H34M" }, "type": "interval" },
      { "foo": [], "type": "emptyarray" },
      { "foo": {}, "type": "emptyobject" },
      { "foo": [15, [{ "baz": ["quux"] }]] },
      { "foo": { "bar": 15, "baz": ["qx"] } },
      { "foo": "nope", "type": "number" },
      { "foo": "nope", "type": "number" }
    ]
}
