{
    "name": "[qa_s07] order join by alias",

    "backends": {
        "mimir":             "ignoreFieldOrder",
        "lwc_local":         "ignoreFieldOrder"
    },


    "data": ["../smallZips.data", "../zips.data"],

    "query": "select z1.`_id` as zip, z2.pop / 1000 as popK
              from `../smallZips.data` as z1 inner join `../zips.data` as z2
              on z1.`_id` = z2.`_id`
              order by popK desc",

