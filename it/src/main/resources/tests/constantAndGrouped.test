{
    "name": "constant and a grouped value",

    "backends": {
        "mimir":          "ignoreFieldOrder"
    },
    "data": "zips.data",

    "variables": {
        "state": "\"CO\""
    },

    "query": "select :state as state, count(*) as `count` from `zips.data` where state = :state",

    "predicate": "exactly",
    "ignoreResultOrder": true,
    "expected": [{ "state": "CO", "count": 414 }]
}
